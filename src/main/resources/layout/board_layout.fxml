<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.dudududi.grainexpansion.controller.BoardController">
    <ImageView fx:id="boardView" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true"/>
    <ImageView fx:id="energyView" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true"/>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
    </padding>
</VBox>
