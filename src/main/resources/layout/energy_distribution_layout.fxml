<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minWidth="300.0" prefHeight="37.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dudududi.grainexpansion.controller.StructuresController">
   <children>
       <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
          <children>
              <Label text="Energy distribution:" />
              <ComboBox fx:id="selectStructureType" minWidth="150.0" prefHeight="30.0" prefWidth="150.0" />
          </children>
       </HBox>
       <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
          <children>
              <Label fx:id="boundariesSizeLabel" text="Energy value:" />
              <TextField fx:id="boundariesSizeField" prefWidth="150.0" />
          </children>
       </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label fx:id="boundariesSizeLabel1" text="Energy value 2:" />
            <TextField fx:id="boundariesSizeField1" prefWidth="150.0" />
         </children>
      </HBox>
       <Button fx:id="colorAllBoundariesButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="250.0" text="Color all boundaries" />
   </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
