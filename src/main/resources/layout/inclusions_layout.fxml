<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxWidth="300.0" prefHeight="158.0" prefWidth="300.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.dudududi.grainexpansion.controller.InclusionsController">
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
        <Label text="Inclusions amount:"/>
        <TextField fx:id="inclusionsAmountField" prefHeight="30.0" prefWidth="150.0"/>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
        <Label text="Inclusions size:"/>
        <TextField fx:id="inclusionsSizeField" prefHeight="30.0" prefWidth="150.0"/>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="28.0">
        <Label text="Inclusions type:"/>
        <ComboBox fx:id="selectInclusionsType" prefHeight="30.0" prefWidth="150.0"/>
    </HBox>
    <Button fx:id="addInclusionsButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="250.0"
            text="Add inclusions"/>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
