<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minWidth="300.0" prefHeight="56.0" prefWidth="300.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.dudududi.grainexpansion.controller.StructuresController">
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
        <Label text="Structure:"/>
        <ComboBox fx:id="selectStructureType" minWidth="150.0" prefHeight="30.0" prefWidth="150.0"/>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
        <Label fx:id="boundariesSizeLabel" text="Boundaries size:"/>
        <TextField fx:id="boundariesSizeField" prefWidth="150.0"/>
    </HBox>
    <Button fx:id="colorAllBoundariesButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="250.0"
            text="Color all boundaries"/>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
